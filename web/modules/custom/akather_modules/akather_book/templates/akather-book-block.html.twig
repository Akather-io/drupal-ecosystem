{% set description_value = data.description.value %}
{%  if description_value|length == 0 %}
  {% set description_value = ' ' %}
{%  endif %}
{% set description = {
      '#type':   'processed_text',
      '#text':    description_value,
      '#format':  data.description.format,
    }
%}
<div>
{{ data.title }}
</div>
<div>
{{ description }}
</div>
<div class="webgl-content" style="display: table; width: 100%; height:100%">
  <div id="unityContainer" style="width: 1920; height: 1080"></div>
  </div>
</div>
